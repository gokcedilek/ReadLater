{
  "manifest_version": 2,
  "name": "MyExtension",
  "version": "1",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "books.jpeg",
    "default_popup": "popup.html"
  },
  "permissions": ["tabs", "storage", "bookmarks"]
}
